{
    "initial_working_directory": "~",
    "font_size_points": 13,
    "editor_theme": "catppuccin-macchiato",
    "posix_terminal_shell": "zsh",
    "jobs_tab_visibility": "shown",
    "auto_detect_indentation": true,
    "insert_native_pipe_operator": true,
    "soft_wrap_r_files": true,
    "highlight_selected_line": true,
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "show_diagnostics_other": true,
    "indent_guides": "rainbowfills",
    "syntax_color_console": true,
    "panes": {
        "quadrants": [
            "Source",
            "Console",
            "TabSet1",
            "TabSet2"
        ],
        "tabSet1": [
            "Environment",
            "History",
            "Build",
            "VCS",
            "Tutorial",
            "Presentation"
        ],
        "tabSet2": [
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [
            "Connections"
        ],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "check_arguments_to_r_function_calls": true,
    "check_unexpected_assignment_in_function_call": true,
    "style_diagnostics": true,
    "continue_comments_on_newline": true,
    "show_invisibles": true,
    "show_help_tooltip_on_idle": true,
    "tab_multiline_completion": true,
    "auto_expand_error_tracebacks": true,
    "console_double_click_select": true,
    "help_font_size_points": 13,
    "show_rmd_render_command": true,
    "visual_markdown_editing_show_margin": true,
    "visual_markdown_code_editor_line_numbers": true,
    "code_completion_include_already_used": true,
    "warn_if_no_such_variable_in_scope": true,
    "warn_variable_defined_but_not_used": true,
    "code_formatter": "styler",
    "python_type": "system",
    "python_version": "3.13.5",
    "python_path": "/opt/homebrew/bin/python3",
    "copilot_enabled": true,
    "default_project_location": "~/Documents/01-livs/20-work/iMTAH-capacitaciones/metabolomica/tarea-final/metaboanalist",
    "reformat_on_save": true,
    "auto_append_newline": true,
    "strip_trailing_whitespace": true
}